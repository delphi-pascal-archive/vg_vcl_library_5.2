  vgBDE in 'vgBDE.pas',
  ExplrDB in 'ExplrDB.pas',
  ExplrBDE in 'ExplrBDE.pas',
  vgWPDB in 'vgWPDB.pas',
  vgWPBDE in 'vgWPBDE.pas',
  vgCanExp in 'vgCanExp.pas',
  vgDBExpr in 'vgDBExpr.pas',
